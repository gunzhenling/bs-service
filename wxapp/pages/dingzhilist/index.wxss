/* pages/dingzhilist/index.wxss */
page {
  width: 100%;
  background: #ffffff;
}

.package {
  width: 100%;
  height: 480rpx;
  background: #ffffff;
  margin-top: 16rpx;
  overflow: hidden
}

.describe {
  width: 80%;
  height: 26rpx;
  display: block;
  line-height: 26rpx;
  margin: 50rpx 0 30rpx 24rpx;
  font-size: 26rpx;
  color: #999999;
}

.info {
  width: 94%;
  height: 76rpx;
  margin: auto;
  display: flex;
  justify-content: space-between;
}

.input_box {
  width: 338rpx;
  height: 76rpx;
  background: #f4f4f4;
  display: flex;
  justify-content: space-between;
  border-radius: 2rpx;
}

.property {
  width: 76rpx;
  height: 76rpx;
  text-align: center;
  line-height: 76rpx;
  font-size: 30rpx;
  font-weight: bold;
}

.num {
  width: 174rpx;
  height: 76rpx;
  display: block;
  margin-right: 24rpx;
  font-size: 24rpx;
  text-align: right
}

.right {
  font-size: 24rpx;
}

.kong {
  width: 100%;
  height: 22rpx;
}

.unex_property {
  width: 194rpx;
  height: 76rpx;
  text-align: center;
  line-height: 76rpx;
  font-size: 30rpx;
  font-weight: bold;
}

.storage {
  width: 94%;
  height: 78rpx;
  margin-top: 30rpx;
  border-radius: 39rpx;
  border: none;
  display: block;
  background: #5a5d7a;
  line-height: 78rpx;
  color: #ffffff;
  font-size: 36rpx;
  font-weight: bold
}

.baoxian {
  display: block;
  margin-top: 22rpx;
  width: 100%;
  height: 28rpx;
  font-size: 28rpx;
  line-height: 28rpx;
  text-align: center;
  color: #cccccc;
}

.ad {
  width: 100%;
  height: 254rpx;
  display: block;
  margin-top: 16rpx;
}

.serve {
  width: 100%;
  height: 534rpx;
  background: #ffffff;
  margin-top: 16rpx;
  overflow: hidden
}

.serve_head {
  width: 94%;
  height: 32rpx;
  margin: auto;
  margin-top: 48rpx;
  display: flex;
  justify-content: space-between
}

.line_left {
  width: 200rpx;
  height: 2rpx;
  background: linear-gradient(#ffffff, #e6e6e6);
  margin-top: 16rpx;
}

.line_right {
  width: 200rpx;
  height: 2rpx;
  background: linear-gradient(#e6e6e6, #ffffff);
  margin-top: 16rpx;
}

.serve_title {
  width: 208rpx;
  height: 32rpx;
  display: flex;
  justify-content: space-between;
  overflow: hidden
}

.serve_title_pic {
  width: 32rpx;
  height: 28rpx;
  display: block;
  margin-top: 2rpx;
}

.serve_title_word {
  font-size: 32rpx;
  color: #3f4865;
  font-weight: bold;
  line-height: 32rpx;
}

.guocheng {
  width: 640rpx;
  height: 204rpx;
  margin: auto;
  display: flex;
  justify-content: space-around;
  margin-top: 60rpx;
}

.cun {
  width: 134rpx;
  height: 122rpx;
  margin-top: 60rpx;
}

.yunsong {
  width: 172rpx;
  height: 204rpx;
}

.dian {
  width: 88rpx;
  height: 6rpx;
  margin-top: 112rpx;
}

.qu {
  width: 134rpx;
  height: 122rpx;
  margin-top: 60rpx;
}

.step {
  width: 94%;
  height: 128rpx;
  margin: auto;
  display: flex;
  justify-content: space-between;
}

.steps {
  width: 192rpx;
  height: 100%;
  overflow: hidden
}

.step_text1 {
  display: block;
  width: 192rpx;
  height: 20rpx;
  line-height: 20rpx;
  font-size: 20rpx;
  text-align: center;
  font-weight: bold;
  color: #5c5f7c;
  margin-top: 20rpx;
}

.step_text2 {
  width: 192rpx;
  height: 24rpx;
  display: block;
  font-size: 24rpx;
  line-height: 24rpx;
  margin-top: 22rpx;
  text-align: center;
  color: #333333;
}

.step_text3 {
  width: 192rpx;
  height: 22rpx;
  display: block;
  font-size: 22rpx;
  line-height: 22rpx;
  margin-top: 20rpx;
  text-align: center;
  color: #999999;
}

.dingzhi-list {
  padding-bottom: 4rem;
}

.dingzhi-item {
  border-bottom: 1px solid #dddddd;
  position: relative;
}

.dingzhi-img {
  width: 100%;
  height: 80px;
}

.dintzhi-price {
  background-color: #ed3f14;
  color: #ffffff;
  border-radius:3px;
  height:30px;
  width:100px;
  line-height:30px;
  text-align: center;
  position:absolute;
  display: block;
  bottom:3px;
  right:3px;
  opacity: 0.8;
}
.dingzhi-status-shouwan{
  position:absolute;
  display: block;
  top:6px;
  right:3px;
  width:40px;
  height:40px;
}